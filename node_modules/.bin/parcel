#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules/parcel-bundler/bin/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules/parcel-bundler/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules/parcel-bundler/bin/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules/parcel-bundler/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/registry.npmmirror.com+parcel-bundler@1.12.5/node_modules:/Users/zhangxinxin/Desktop/threejs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parcel-bundler/bin/cli.js" "$@"
else
  exec node  "$basedir/../parcel-bundler/bin/cli.js" "$@"
fi
